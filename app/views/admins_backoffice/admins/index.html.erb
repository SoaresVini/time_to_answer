<h1>Listando Admins</h1>
<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
  <thead>
    <tr>
      <th>#</th>
      <th>Email</th>
      <th>
        <%= link_to '', new_admins_backoffice_admin_path, class: 'btn btn-success btn-circle' %>
      </th>
    </tr>
  </thead>
  <tbody>
    <% @admins.each do |admin|%>
    <tr>
      <td><%= admin.id %></td>
      <td><%= admin.email %></td>
      <td class="list__actions">
        <%= link_to '', edit_admins_backoffice_admin_path(admin), class: 'btn btn-primary btn-circle' %>
        <%= button_to '', admins_backoffice_admin_path(admin), method: :delete, class: 'btn btn-danger btn-circle' %>
      </td>
    </tr>
    <%end%>
  </tbody>

</table>
<div class="text-center">
 <%= paginate @admins %>
 </div>
